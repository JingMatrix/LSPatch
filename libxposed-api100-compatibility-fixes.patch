From 2f692850782c7ae49b15234ed4083ccf70345f15 Mon Sep 17 00:00:00 2001
From: zivzhao <zivzhao@icloud.com>
Date: Sat, 31 May 2025 23:00:35 +0800
Subject: [PATCH 2/2] Configure build environment for libxposed api 100
 compatibility

---
 build.gradle.kts    | 2 +-
 core                | 2 +-
 gradle.properties   | 8 ++++++++
 settings.gradle.kts | 2 ++
 4 files changed, 12 insertions(+), 2 deletions(-)

diff --git a/build.gradle.kts b/build.gradle.kts
index a4ff58e..6d45dac 100644
--- a/build.gradle.kts
+++ b/build.gradle.kts
@@ -81,7 +81,7 @@ fun Project.configureBaseExtension() {
         buildToolsVersion = androidBuildToolsVersion
 
         externalNativeBuild.cmake {
-            version = "3.28.1+"
+            version = "3.31.6"
             buildStagingDirectory = layout.buildDirectory.get().asFile
         }
 
diff --git a/core b/core
index 90d3712..16890d8 160000
--- a/core
+++ b/core
@@ -1 +1 @@
-Subproject commit 90d3712c6e99199ae9797de15710dd12c4b107e4
+Subproject commit 16890d87c425d6ca36edf8746f1e7a4842ba58fc
diff --git a/gradle.properties b/gradle.properties
index 387da39..55fe0a1 100644
--- a/gradle.properties
+++ b/gradle.properties
@@ -1,3 +1,11 @@
 android.experimental.enableNewResourceShrinker.preciseShrinking=true
 android.enableAppCompileTimeRClass=true
 android.useAndroidX=true
+
+# 增加Gradle内存配置
+org.gradle.jvmargs=-Xmx8g -XX:MaxMetaspaceSize=1g -XX:+HeapDumpOnOutOfMemoryError
+org.gradle.parallel=true
+org.gradle.caching=true
+
+# 强制使用支持C++20模块的CMake版本
+android.cmake.version=3.31.6
diff --git a/settings.gradle.kts b/settings.gradle.kts
index 9364f78..d6abd97 100644
--- a/settings.gradle.kts
+++ b/settings.gradle.kts
@@ -38,6 +38,7 @@ include(
     ":hiddenapi:bridge",
     ":hiddenapi:stubs",
     ":jar",
+    ":libxposed-compat",
     ":manager",
     ":meta-loader",
     ":patch",
@@ -54,6 +55,7 @@ project(":axml").projectDir = file("core/axml")
 project(":core").projectDir = file("core/core")
 project(":hiddenapi:bridge").projectDir = file("core/hiddenapi/bridge")
 project(":hiddenapi:stubs").projectDir = file("core/hiddenapi/stubs")
+project(":libxposed-compat").projectDir = file("core/libxposed-compat")
 project(":services:daemon-service").projectDir = file("core/services/daemon-service")
 project(":services:manager-service").projectDir = file("core/services/manager-service")
 project(":services:xposed-service:interface").projectDir = file("core/services/xposed-service/interface")
